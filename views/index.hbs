         {{#each products }}
          {{#each this}}
          <div class="col-sm">
  <div class="card" style="width: 20rem;">
    <img class="card-img-top" src="{{this.imagePath}}"alt="Card image cap" class="img-responsive">
  <div class="card-body">
    <h4 class="card-title">{{this.title}} </h4>
    <p class="card-text">{{this.description}}</p>
   <div class="clear-fix">
        <div class="price pull-left">${{this.price}} </div>
        <a href="#" class="btn btn-success pull-right">Buy</a>
   </div>

  </div>
  </div>
</div>
          {{/each}}
         {{/each}}

